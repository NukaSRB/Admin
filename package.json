      ]
    },
    "bootstrap-dropdown": {
      "depends": [
        "jquery"
      ]
    },
    "bootstrap-modal": {
      "depends": [
        "jquery"
      ]
    },
    "bootstrap-offcanvas": {
      "depends": [
        "jquery"
      ]
    },
    "bootstrap-popover": {
      "depends": [
        "jquery"
      ]
    },
    "bootstrap-scrollspy": {
      "depends": [
        "jquery"
      ]
    },
